#version 450

layout (local_size_x = 32) in;

layout(set = 0, binding = 0) readonly  buffer bina { float in_a[]; };
layout(set = 0, binding = 1) readonly  buffer binb { float in_b[]; };
layout(set = 0, binding = 2) writeonly buffer bout { float result[]; };

void main() {
    const uint index = gl_GlobalInvocationID.x;
    result[index]    = float(in_a[index] == in_b[index]);
}